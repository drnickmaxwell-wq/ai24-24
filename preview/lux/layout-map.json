        "breakpoints": {
          "mobile": "px-6",
          "desktop": "px-12"
        },
        "effects": ["wave-patterns", "floating-particles", "gradient-text"]
      },
      {
        "id": "treatments",
        "name": "Luxury Coastal Dentistry",
        "order": 3,
        "spacing": "py-32",
        "description": "Treatment cards with circular gradient icons: 3D Digital Dentistry, Porcelain Veneers, Dental Implants",
        "breakpoints": {
          "mobile": "grid-cols-1",
          "desktop": "grid-cols-3"
        },
        "effects": ["glow-cards", "hover-animations", "gradient-buttons"]
      },
      {
        "id": "stats",
        "name": "Practice Statistics",
        "order": 4,
        "spacing": "py-16",
